{
    "swagger": "2.0",
    "info": {
      "description": "API FOR EVENTPOINTS\n",
      "version": "0.0.1",
      "title": "EventPoints API",
      "contact": {
        "name": "OSWeekends",
        "url": "http://www.osweekends.com"
      }
    },
    "basePath": "/api/v1",
    "schemes": ["http"],
    "paths": {
      "/": {
        "get": {
          "tags": ["Ping"],
          "description": "Gets API up and runing messages\n",
          "produces": ["application/json"],
          "parameters": [],
          "responses": {
            "200": {
              "description": "API up and running message",
              "schema": {
                "$ref": "#/definitions/inline_response_200"
              }
            }
          }
        }
      },
      "/sources": {
        "get": {
          "tags": ["Sources"],
          "description": "Gets all sources for tech events\n",
          "produces": ["application/json"],
          "responses": {
            "200": {
              "description": "Sources propperly fetched",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/events_source"
                }
              }
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events": {
        "get": {
          "tags": ["Events"],
          "description": "Gets all tech events\n",
          "produces": ["application/json"],
          "parameters": [
            {
            "in": "query",
            "name": "from",
            "type": "string",
            "description": "Fecha inicio  para filtrar eventos"
            },
            {
              "in": "query",
              "name": "to",
              "type": "string",
              "description": "Fecha fin  para filtrar eventos"
              }
          ],
          "responses": {
            "200": {
              "description": "Events propperly fetched",
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/definitions/inline_response_200_1"
                }
              }
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      },
      "/events/{id}": {
        "get": {
          "tags": ["Events"],
          "description": "Gets tech event details by Id\n",
          "produces": ["application/json"],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "description": "ID of event to fetch",
              "required": true,
              "type": "string"
            }
          ],
          "responses": {
            "200": {
              "description": "Event propperly fetched",
              "schema": {
                "$ref": "#/definitions/inline_response_200_1"
              }
            },
            "404": {
              "description": "Event not found"
            },
            "500": {
              "description": "Internal Server Error"
            }
          }
        }
      }
    },
